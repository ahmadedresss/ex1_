#include <stdlib.h>
#include <string.h>
#include <assert.h>

char* stringDuplicator(char* str, int times)
{
assert(s);
assert(times > 0);
int len = strlen(str);
char* out = malloc(sizeof(*out)*((len * times) + 1));
assert(out);
char* ptr=out;
strcpy(ptr,str);
 for(int i=0; i<times; i++)
 {
    ptr=ptr+len;
     strcpy(ptr,s);
  }
    return out;
}
