#include <stdlib.h>
#include <string.h>
#include <assert.h>

char* stringDuplicator(char* s, int times)
{
assert(s);
assert(times > 0);
int LEN = strlen(s);
char* out = malloc(sizeof(*out)*((LEN * times) + 1));
assert(out);
